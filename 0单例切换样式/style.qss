    /*pm*/color: white;
    /*am*/color: black;
}

/*QPushButton*/
QPushButton{
        font-size: 22px;
        font-family: 'Microsoft YaHei';
        /*pm*/background: #505050;
        /*am*/background: white;
        /*pm*/color: white;
        /*am*/color: #333333;
        /*am*/border: 1.5px solid #D7D8DC;
        border-radius: 6px;
        padding-top: 5px;
        padding-bottom: 5px;
}
QPushButton:checked,QPushButton:pressed{
        background: #0088ff;
        /*am*/color: white;
}
QPushButton:!enabled{
        /*pm*/color: #999999;
        /*am*/color: #222222;
}

/*QScrollBar*/
QScrollBar{
    /*pm*/background-color: black;
    /*am*/background-color: #dfdfdf;
    /*pm*/border: 2px solid black;
    /*am*/border: 2px solid #dfdfdf;
    border-radius: 7px;
    padding: 1px;
    height: 15px;
    width: 15px;
}
QScrollBar::handle{
    /*pm*/background: #797979;
    /*am*/background: white;
    /*pm*/border: 2px solid #797979;
    /*am*/border: 2px solid white;
    border-radius: 4px;
}
QScrollBar::add-line, QScrollBar::sub-line,
QScrollBar::add-page, QScrollBar::sub-page {
    width: 0px;
    background: transparent;
}

/*QSpliter*/
QSplitter::handle:horizontal {
    width: 5px;
    /*pm*/background-color: #676767;
    /*am*/background-color: #d7d8dc;
}
QSplitter::handle:pressed {
    background-color:grey;
}

/*QTreeWidget*/
QTreeWidget{
    font: 18px 'Microsoft YaHei';
    /*pm*/background-color: black;
    /*am*/background-color: white;
    /*pm*/color: #adadad;
    /*am*/color: black;
    border:none;
}
QTreeWidget::item
{
    min-width: 60px;
    min-height: 40px;
}
QTreeWidget::item:selected:active,
QTreeWidget::item:selected:!active,
QTreeWidget::item:hover
{
    /*pm*/background: #797979;
    /*am*/background: #0088ff;
}

QTreeWidget::indicator
{
    width:  15px;
    height: 15px;
    padding:0px;
}
QTreeWidget::indicator:checked {
        /*pm*/image: url(:/image/night/main_right/checked.png);
        /*am*/image: url(:/image/day/main_right/checkLight.png);
        /*pm*/background-color: #1E1E1E;
        /*am*/background-color: white;
}
QTreeWidget::indicator:unchecked {
        /*pm*/image: url(:/image/night/main_right/uncheck.png);
        /*am*/image: url(:/image/day/main_right/uncheckLight.png);
        /*pm*/background-color: black;
        /*am*/background-color: white;
}

QTreeWidget::branch:has-siblings:!adjoins-item {
    /*pm*/border-image: url(:/image/night/main_right/vline-white.png) 0;
    /*am*/border-image: url(:/image/day/main_right/vline-black.png) 0;
}
QTreeWidget::branch:has-siblings:adjoins-item {
    /*pm*/border-image: url(:/image/night/main_right/branch-more-white.png) 0;
    /*am*/border-image: url(:/image/day/main_right/branch-more-black.png) 0;
}
QTreeWidget::branch:!has-children:!has-siblings:adjoins-item {
    /*pm*/border-image: url(:/image/night/main_right/branch-end-white.png) 0;
    /*am*/border-image: url(:/image/day/main_right/branch-end-black.png) 0;
}
QTreeWidget::branch:has-children:!has-siblings:closed,
QTreeWidget::branch:closed:has-children:has-siblings {
    border-image: none;
    /*pm*/image: url(:/image/night/main_right/branch-closed-white.png);
    /*am*/image: url(:/image/day/main_right/branch-closed-black.png);
}
QTreeWidget::branch:open:has-children:!has-siblings,
QTreeWidget::branch:open:has-children:has-siblings  {
    border-image: none;
    /*pm*/image: url(:/image/night/main_right/branch-open-white.png);
    /*am*/image: url(:/image/day/main_right/branch-open-black.png);
}
