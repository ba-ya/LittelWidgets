/*QComboBox*/
QComboBox{
    font: 24px "Microsoft YaHei";
    /*pm*/color: white;
    /*am*/color: #333333;
    /*pm*/background: black;
    /*am*/background: white;
    /*am*/border: 1.5px solid #D7D8DC;
    /*pm*/height: 42px;
    /*am*/height: 38px;
    border-radius: 6px;
    padding-left: 15px;
    padding-right: 0px;
}
QComboBox::down-arrow{
        /*pm*/image: url(:/image/share/down_white.png);
        /*am*/image: url(:/image/share/down_black.png);
}
QComboBox::drop-down{
        width: 30px;
        border: none;
}
QComboBox QAbstractItemView{
        font: 23px "Microsoft YaHei";
        outline: 0px;
        /*pm*/background: black;
        /*am*/background: white;
        /*pm*/color: #80FFFFFF;
        /*pm*/border: 1.2px solid #4B4B4B;
        /*am*/color: #80333333;
        /*am*/border: 1.2px solid #B6B6B6;
        border-top: 0px;
        border-bottom-left-radius: 6px;
        border-bottom-right-radius: 6px;
        padding-left: 10px;
}

/*DoubleSpinBox*/
QDoubleSpinBox{
        font-size: 24px;
        font-family: 'Microsoft YaHei';
        /*pm*/background-color: black;
        /*am*/background-color: #d7d8dc;
        /*pm*/color: white;
        /*am*/color: #333333;
        padding-top: 2%;
        padding-bottom: 2%;
        padding-left: 6%;
        /*pm*/border: 4px solid black;
        /*am*/border: 4px solid #d7d8dc;
        border-radius: 10px;
}
QDoubleSpinBox:down-button,QDoubleSpinBox:up-button
{
    width: 0px;
}
QDoubleSpinBox:!enabled{
        /*pm*/color: #80FFFFFF;
        /*am*/color: #80333333;
}


/*QLabel*/
QLabel{
    font-size: 18px;
    font-family: "Microsoft YaHei";
    padding: 0px;
    padding-bottom: 2%;
    /*pm*/background-color: #1e1e1e;
    /*am*/background-color: white;
    /*pm*/color: #adadad;
    /*am*/color: #8C8C8C;
}

/*QLineEdit*/
QLineEdit{
        font: 24px "Microsoft YaHei";
        /*pm*/background-color: black;
        /*am*/background-color: white;
        /*pm*/color:white;
        /*am*/color:black;
        /*pm*/border: 1.5px solid black;
        /*am*/border: 1.5px solid #D7D8DC;
        padding:0px;
        border-radius: 6px;
        padding-left: 18px;
        height: 38px;
}

/*QMessageBox*/
QMessageBox {
    /*pm*/background: grey;
    /*am*/background: white;
    /*pm*/color: white;
    /*am*/color: black;
}
QMessageBox QLabel{
    font: 23px "Microsoft YaHei";
